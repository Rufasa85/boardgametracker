<h1>Games!</h1>
<% plays.forEach(function(play){  %>
<a href='/plays/<%= play.id %>'><div class="well">
    <h1><%=play.game%></h1>
    <p><strong>Winner: <%=play.winner%></strong></p>
    <p> players:</p>
    <ul>
        <% play.players.forEach(function(player) { %>
        <li>
            <%= player %>
        </li>
        <% }) %>
    </ul>
</div> </a>
<% }) %>
